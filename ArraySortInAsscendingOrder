#include<bits/stdc++.h>
using namespace std;


int main(){
    int array[]={458,12,56,4,1};
    for(int i =0;i<5;i++){
        for (int j = i+1;j<5;j++){
            if(array[i]>array[j]){
                int temp =array[i];
                array[i]=array[j];
                array[j]=temp; 
               
            }
        }
         cout<<array[i]<<"\t";
    }

    return 0;
}
